{
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "app.py" }
    ],
    "env": {
      "MYSQL_HOST": "@mysql_host",
      "MYSQL_USER": "@mysql_user",
      "MYSQL_PASSWORD": "@mysql_password",
      "MYSQL_DB": "@mysql_db",
      "SECRET_KEY": "@secret_key"
    }
  }
  